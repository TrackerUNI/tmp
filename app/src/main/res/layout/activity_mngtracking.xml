<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btnmngAccount2"
        android:layout_width="136sp"
        android:layout_height="wrap_content"
        android:text="Admin Cuenta"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="683dp" />

    <Button
        android:id="@+id/btnMapa2"
        android:layout_width="136sp"
        android:layout_height="wrap_content"
        android:text="Mapa"
        app:layout_constraintStart_toEndOf="@+id/btnmngAccount2"
        tools:layout_editor_absoluteY="683dp" />

    <Button
        android:id="@+id/btnmngTracking2"
        android:layout_width="138dp"
        android:layout_height="48dp"
        android:text="Admin Tracking"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/btnMapa2"
        tools:layout_editor_absoluteY="683dp" />
</androidx.constraintlayout.widget.ConstraintLayout>